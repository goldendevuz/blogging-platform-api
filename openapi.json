  /categories/:
    get:
      operationId: categories_list
      parameters:
      - name: term
        required: false
        in: query
        description: A search term.
        schema:
          type: string
      tags:
      - categories
      security:
      - cookieAuth: []
      - basicAuth: []
      - {}
