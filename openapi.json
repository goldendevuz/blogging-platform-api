components:
  schemas:
    Category:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        created:
          type: string
          format: date-time
          readOnly: true
        modified:
          type: string
          format: date-time
          readOnly: true
        title:
          type: string
          maxLength: 100
      required:
      - created
      - id
      - modified
      - title
    PatchedCategory:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        created:
          type: string
          format: date-time
          readOnly: true
        modified:
          type: string
          format: date-time
          readOnly: true
        title:
          type: string
          maxLength: 100
    PatchedPost:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        category:
          type: integer
        tags:
          type: array
          items:
            type: string
        created:
          type: string
          format: date-time
          readOnly: true
        modified:
          type: string
          format: date-time
          readOnly: true
        title:
          type: string
          maxLength: 100
        content:
          type: string
    Post:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        category:
          type: integer
        tags:
          type: array
          items:
            type: string
        created:
          type: string
          format: date-time
          readOnly: true
        modified:
          type: string
          format: date-time
          readOnly: true
        title:
          type: string
          maxLength: 100
        content:
          type: string
      required:
      - category
      - content
      - created
      - id
      - modified
      - tags
      - title
  securitySchemes:
    basicAuth:
      type: http
      scheme: basic
    cookieAuth:
      type: apiKey
      in: cookie
      name: sessionid
